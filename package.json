{
  "name": "spotify-wrapper-player",
  "version": "1.0.0",
  "description": "A player using the spotify-wrapper lib.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "prepush": "npm run lint && npm test:coverage",
    "start": "./node_modules/.bin/webpack-dev-server"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CheeseFlan/spotify-wrapper-player.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Joao Henrique Gomes <joaoh3326@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CheeseFlan/spotify-wrapper-player/issues"
  },
  "homepage": "https://github.com/CheeseFlan/spotify-wrapper-player#readme",
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "standard": "^10.0.3",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "spotify-wrapper": "^2.0.2"
  }
}
